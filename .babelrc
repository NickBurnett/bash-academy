{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            { "regenerator": true }
        ],
        [
            "module-resolver",
            {
                "root": [
                    "./front/**"
                ],
                "alias": {
                    "@": "./front",
                    "@font": "./assets/font",
                    "@component": "./front/js/component",
                    "@style": "./front/css"
                }
            }
        ]
    ]
}